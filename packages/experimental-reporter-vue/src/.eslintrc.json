{
  "extends": [
    "plugin:@cypress/dev/react",
    "plugin:@cypress/dev/tests"
  ],
  "parser": "@typescript-eslint/parser",
  "settings": {
    "react": {
      "version": "16.12"
    }
  },
  "plugins": [
    "cypress"
  ],
  "env": {
    "cypress/globals": true
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-no-bind": "off",
    "arrow-body-style": "off",
    "no-unused-vars": "off",
    "react/"
    "react/jsx-filename-extension": [
      "warn",
      {
        "extensions": [
          ".js",
          ".jsx",
          ".tsx"
        ]
      }
    ]
  }
}
