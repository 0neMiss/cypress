{
  "name": "@packages/launchpad",
  "version": "0.0.0-development",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf ./node_modules/.vite && yarn codegen",
    "build": "vite build",
    "types": "vue-tsc --noEmit",
    "build-prod": "cross-env NODE_ENV=production yarn build",
    "clean-deps": "rm -rf node_modules",
    "test": "yarn cypress:run && yarn types",
    "build-for-e2e": "kill -9 `lsof -t -i:52100` || echo ok && yarn test:e2e:serve & yarn build-prod",
    "precypress:open:e2e": "yarn build-for-e2e",
    "cypress:open:e2e": "LAUNCHPAD=1 CI=true node ../../scripts/cypress.js open",
    "precypress:run:e2e": "yarn build-for-e2e",
    "cypress:run:e2e": "LAUNCHPAD=1 CI=true node ../../scripts/cypress.js run",
    "cypress:open:ct": "LAUNCHPAD=1 CI=true yarn build-prod && node ../../scripts/cypress.js open-ct",
    "cypress:run:ct": "LAUNCHPAD=1 CI=true yarn build-prod && node ../../scripts/cypress.js run-ct",
    "cypress:run": "yarn cypress:run:e2e && yarn cypress:run:ct",
    "cypress:open": "LAUNCHPAD=1 node ../../scripts/cypress.js open",
    "test:e2e:serve-frontend": "VITE_CYPRESS_INTERNAL_GQL_PORT=52100 yarn build-prod --outDir=./dist-e2e && yarn serve ./dist-e2e -l 5000",
    "test:e2e:serve": "concurrently \"yarn test:e2e:serve-frontend\" \"yarn test:e2e:start-gql\"",
    "test:e2e:start-gql": "CYPRESS_INTERNAL_GQL_PORT=52100 CYPRESS_INTERNAL_E2E_TESTING_SELF=1 NODE_ENV=development LAUNCHPAD=1 node ../../scripts/cypress open",
    "dev": "NODE_ENV=development LAUNCHPAD=1 node ../../scripts/cypress open # Will open Launchpad against live Vite server",
    "postinstall": "rm -rf ./node_modules/.vite && yarn codegen && echo '@packages/launchpad needs: yarn build'",
    "start": "vite --port 3001",
    "watch": "concurrently \"yarn start\" \"yarn codegen --watch\"",
    "codegen": "graphql-codegen --config ${PWD}/graphql-codegen.yml"
  },
  "dependencies": {
    "@iconify/json": "1.1.368",
    "@iconify/vue": "3.0.0-beta.1",
    "bluebird": "3.5.3",
    "classnames": "2.3.1",
    "vue": "3.2.0"
  },
  "devDependencies": {
    "@cypress/vue": "0.0.0-development",
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.6",
    "@graphql-codegen/typed-document-node": "^1.18.9",
    "@graphql-codegen/typescript": "^1.22.4",
    "@graphql-codegen/typescript-operations": "^1.18.3",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@headlessui/vue": "1.4.0",
    "@iconify/json": "1.1.368",
    "@iconify/vue": "3.0.0-beta.1",
    "@intlify/vite-plugin-vue-i18n": "2.4.0",
    "@testing-library/cypress": "8.0.0",
    "@urql/core": "2.1.5",
    "@urql/exchange-execute": "^1.0.4",
    "@urql/vue": "0.4.3",
    "@vitejs/plugin-vue": "1.2.4",
    "@vitejs/plugin-vue-jsx": "1.1.6",
    "@vueuse/core": "5.2.0",
    "bluebird": "3.5.3",
    "classnames": "2.3.1",
    "concurrently": "^6.2.0",
    "cross-env": "6.0.3",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.5",
    "javascript-time-ago": "2.3.8",
    "prismjs": "1.24.0",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "serve-static": "1.14.1",
    "vite": "2.4.4",
    "vite-plugin-components": "0.11.3",
    "vite-plugin-icons": "0.6.3",
    "vite-plugin-purge-icons": "0.7.0",
    "vite-plugin-windicss": "1.2.4",
    "vite-svg-loader": "2.1.1",
    "vue": "3.2.0",
    "vue-i18n": "9.2.0-beta.1",
    "vue-prism-component": "2.0.0",
    "vue-prism-editor": "^2.0.0-alpha.2",
    "vue-tsc": "^0.3.0",
    "windicss": "3.1.4",
    "wonka": "^4.0.15"
  },
  "files": [
    "dist",
    "lib",
    "script",
    "graphql-codegen.yml",
    "src"
  ]
}
