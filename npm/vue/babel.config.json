{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry"
      }
    ],
    "babel-preset-typescript-vue3",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-modules-commonjs",
    [
      "babel-plugin-istanbul",
      {
        "extension": [
          ".js",
          ".vue"
        ],
        "exclude": [
          "**/*.spec.{js,ts}",
          "**/setup/*.vue"
        ]
      }
    ]
  ]
}
