{
  "name": "@cypress/react-scripts-dev-server",
  "version": "0.0.0-development",
  "description": "Launches React Scripts Dev Server for Component Testing",
  "main": "index.js",
  "scripts": {
    "build": "tsc || echo 'built, with type errors'",
    "build-prod": "tsc || echo 'built, with type errors'",
    "check-ts": "tsc --noEmit",
    "cy:open": "node ../../scripts/cypress.js open --component --project ${PWD}",
    "cy:run": "node ../../scripts/cypress.js run --component --project ${PWD}",
    "test": "yarn cy:run",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@cypress/webpack-dev-server": "0.0.0-development"
  },
  "files": [
    "dist",
    "index.js",
    "index.d.ts"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cypress-io/cypress.git"
  },
  "homepage": "https://github.com/cypress-io/cypress/tree/master/npm/react-scripts-dev-server#readme",
  "bugs": "https://github.com/cypress-io/cypress/issues/new?template=1-bug-report.md",
  "module": "dist/index.js",
  "publishConfig": {
    "access": "public"
  }
}
